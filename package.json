{
  "name": "remix-stack-app-name",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf build && rimraf public/build && rimraf css",
    "build": "run-s build:*",
    "build:css": "npm run generate:tailwind -- --minify && npm run generate:sass -- --style=compressed",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:remix": "remix dev",
    "dev:tailwind": "npm run generate:tailwind -- --watch",
    "dev:sass": "npm run generate:sass -- --watch --no-source-map",
    "dev:gql": "npm run generate:gql -- --watch",
    "generate:sass": "sass app/styles/:public/css",
    "generate:tailwind": "npx tailwindcss -o ./public/css/tailwind.css",
    "generate:gql": "graphql-codegen --config ./app/.gql/codegen.ts",
    "start": "remix-serve build",
    "format": "prettier --write \"app/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.2",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "@remix-run/serve": "*",
    "dayjs": "^1.11.7",
    "framer-motion": "^9.0.1",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "isbot": "^3.6.5",
    "jsonwebtoken": "^9.0.0",
    "just-safe-get": "^4.2.0",
    "jwt-decode": "^3.1.2",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-select": "^5.7.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/typed-document-node": "^3.0.0",
    "@graphql-codegen/typescript": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^3.0.0",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@tailwindcss/typography": "^0.5.9",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "daisyui": "^2.50.0",
    "dotenv": "^16.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "sass": "^1.58.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16"
  }
}
