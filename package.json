{
  "name": "remix-stack-app-name",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf build && rimraf public/build && rimraf css",
    "build": "run-s build:*",
    "build:css": "npm run generate:tailwind -- --minify && npm run generate:sass -- --style=compressed",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:remix": "remix dev",
    "dev:tailwind": "npm run generate:tailwind -- --watch",
    "dev:sass": "npm run generate:sass -- --watch --no-source-map",
    "dev:gql": "npm run generate:gql -- --watch",
    "generate:sass": "sass app/styles/:public/css",
    "generate:tailwind": "npx tailwindcss -o ./public/css/tailwind.css",
    "generate:gql": "graphql-codegen --config ./app/.gql/codegen.ts",
    "start": "remix-serve build",
    "format": "prettier --write \"app/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.2",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "@remix-run/serve": "*",
    "dayjs": "^1.11.6",
    "framer-motion": "^7.6.4",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "isbot": "^3.5.4",
    "jsonwebtoken": "^8.5.1",
    "just-safe-get": "^4.1.1",
    "jwt-decode": "^3.1.2",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-select": "^5.5.9",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.11",
    "@graphql-codegen/typed-document-node": "^2.3.6",
    "@graphql-codegen/typescript": "^2.8.1",
    "@graphql-codegen/typescript-operations": "^2.5.6",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@tailwindcss/typography": "^0.5.7",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "daisyui": "^2.38.0",
    "dotenv": "^16.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
